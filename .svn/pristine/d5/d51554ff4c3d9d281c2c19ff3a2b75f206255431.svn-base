<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/user_setting_backgroud1" >

    <RelativeLayout
        android:id="@+id/user_setting_rl_top"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:background="@drawable/all_top_backgroup" >

        <ImageView
            android:id="@+id/user_order_iv_top_left"
            style="@style/text_padding_all_10dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/sender_ddress_top_left_bt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/user_setting_top_title"
            android:textSize="22sp" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/user_setting_sc_"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/user_setting_bottom_rl"
        android:layout_below="@+id/user_setting_rl_top"
        android:background="@drawable/user_setting_backgroud1"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/user_setting_rl_1"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_1" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                style="@style/text_padding_lr15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_preferences"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:onClick="rl_preferences"
                android:clickable="true"
                android:layout_height="wrap_content" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/express_preferences_title" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                style="@style/text_padding_lr15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_2"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_2" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dip"
                android:src="@drawable/user_setting_line"
                android:visibility="gone" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_3"
                style="@style/text_padding_lr15_tb3dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_3" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_open" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dip"
                android:src="@drawable/user_setting_line"
                android:visibility="gone" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_4"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_4" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                style="@style/text_padding_lr15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_5"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_5" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                style="@style/text_padding_lr15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line" />

            <RelativeLayout
                android:id="@+id/user_setting_rl_6"
                style="@style/text_padding_all_15dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    style="@style/text_size17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dip"
                    android:text="@string/user_setting_tv_6" />

                <ImageView
                    style="@style/text_padding_all_5dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/user_setting_bt_right" />
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/user_setting_line" />
        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/user_setting_bottom_rl"
        android:layout_width="match_parent"
        android:layout_height="60dip"
        android:layout_alignParentBottom="true"
        android:background="@drawable/user_setting_bt" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/user_setting_quit"
            android:textColor="@color/TextColorWhite"
            android:textSize="20sp" />
    </RelativeLayout>

</RelativeLayout>