<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/reservation_rl_top"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        android:background="@drawable/all_top_backgroup" >

        <ImageView
            android:id="@+id/reservation_iv_top_left"
            style="@style/text_padding_lr15_tb10dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_top_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/reservation_top_title"
            android:textSize="22sp" />
    </RelativeLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/all_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/reservation_rl_top"
        android:layout_marginTop="20dip"
        android:orientation="vertical" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:id="@+id/reservation_ll_select_time"
                style="@style/text_padding_tb_10dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    style="@style/text_padding_lr15_tb10dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/reservation_time" />

                <TextView
                    android:id="@+id/reservation_tv_select_time"
                    style="@style/text_gray_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/reservation_tv_time"
                    android:textSize="17sp" />
            </LinearLayout>

            <ImageView
                style="@style/text_padding_lr15_tb10dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/user_setting_bt_right" />
        </RelativeLayout>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:id="@+id/reservation_ll_select_starting"
                style="@style/text_padding_tb_10dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/reservation_iv_2"
                android:orientation="horizontal" >

                <ImageView
                    style="@style/text_padding_lr15_tb10dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/reservation_starting" />

                <TextView
                    android:id="@+id/reservation_tv_select_starting"
                    style="@style/text_gray_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/reservation_tv_starting"
                    android:textSize="17sp" />

                <EditText
                    android:id="@+id/reservation_et_select_starting"
                    style="@style/etpersonaltbr10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@null"
                    android:ellipsize="end"
                    android:hint="@string/reservation_tv_starting"
                    android:inputType="text"
                    android:lines="1"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textSize="17sp"
                    android:visibility="gone" />
            </LinearLayout>

            <ImageView
                android:id="@+id/reservation_iv_2"
                style="@style/text_padding_lr15_tb10dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/user_setting_bt_right" />
        </RelativeLayout>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:id="@+id/reservation_ll_select_end"
                style="@style/text_padding_tb_10dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/reservation_iv_3"
                android:orientation="horizontal" >

                <ImageView
                    style="@style/text_padding_lr15_tb10dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/reservation_end" />

                <TextView
                    android:id="@+id/reservation_tv_select_end"
                    style="@style/text_gray_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/reservation_tv_end"
                    android:textSize="17sp" />

                <EditText
                    android:id="@+id/reservation_et_select_end"
                    style="@style/etpersonaltbr10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@null"
                    android:ellipsize="end"
                    android:hint="@string/reservation_tv_end"
                    android:inputType="text"
                    android:lines="1"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:textSize="17sp"
                    android:visibility="gone" />
            </LinearLayout>

            <ImageView
                android:id="@+id/reservation_iv_3"
                style="@style/text_padding_lr15_tb10dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/user_setting_bt_right" />
        </RelativeLayout>

        <ImageView
            android:id="@+id/reservation_iv_line"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/all_line" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/reservation_bottom_rl_send"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/index_bottom_backgroup" >

        <Button
            android:id="@+id/reservation_bt_send"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="15dip"
            android:layout_marginRight="15dip"
            android:background="@drawable/index_backgroud1"
            android:text="@string/app_version_confirm"
            android:textColor="@color/TextColorWhite"
            android:textSize="20sp" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/reservation_bottom_ll_wheelview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:visibility="gone" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dip" >

            <TextView
                android:id="@+id/reservation_tv_wv_end1"
                style="@style/text_gray_color_pad5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reservation_tv_wv_end" />

            <TextView
                android:id="@+id/reservation_tv_wv_end"
                style="@style/text_gray_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_toLeftOf="@+id/reservation_bt_wv_confirm"
                android:layout_toRightOf="@+id/reservation_tv_wv_end1"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/reservation_tv_111" />

            <Button
                android:id="@+id/reservation_bt_wv_confirm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/user_order_yuyue"
                android:paddingLeft="5dip"
                android:paddingRight="5dip"
                android:text="@string/app_version_confirm"
                android:textColor="@color/TextColorWhite"
                android:textSize="20sp" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_sheng"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_shi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_qu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/reservation_bottom_ll_wheelview_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:visibility="gone" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="3dip" >

            <Button
                android:id="@+id/reservation_bt_cancel_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@drawable/user_order_yuyue"
                android:paddingLeft="5dip"
                android:paddingRight="5dip"
                android:text="@string/Cancel"
                android:textColor="@color/TextColorWhite"
                android:textSize="20sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_toLeftOf="@+id/reservation_bt_wv_confirm_time"
                android:layout_toRightOf="@+id/reservation_bt_cancel_time"
                android:ellipsize="end"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/reservation_tv_advance" />

            <Button
                android:id="@+id/reservation_bt_wv_confirm_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/user_order_yuyue"
                android:paddingLeft="5dip"
                android:paddingRight="5dip"
                android:text="@string/app_version_confirm"
                android:textColor="@color/TextColorWhite"
                android:textSize="20sp" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />

            <com.striveen.express.wheel.widget.WheelView
                android:id="@+id/reservation_wv_minute"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>